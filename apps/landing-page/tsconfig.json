{
  "extends": "@jwizard-web/typescript-config/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "globals.d.ts",
    "modules.d.ts",
    "next-env.d.ts",
    "next.config",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    ".next/*"
  ]
}
